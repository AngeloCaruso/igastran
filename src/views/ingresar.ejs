<%- contentFor('pageType') %>
<body class="login-page">
<%- contentFor('body') %>
<div>
	<form action="/login" method="POST" enctype='application/json'>
		<input type="text" name="usuario_correo" placeholder="usuario o correo electrónico">
		<input type="text" name="password" placeholder="contraseña">
		<button type="submit">login</button>
	</form>
</div><br>
<div>
	<form action="/registrar" method="POST" enctype="application/json">
		<input type="text" name="nombre" placeholder="Nombre">
		<input type="text" name="apellidos" placeholder="Apellidos">
		<input type="text" name="usuario" placeholder="Usuario">
		<input type="text" name="correo" placeholder="Correo">
		<input type="text" name="password" placeholder="Contraseña">
		<input type="text" name="passwordC" placeholder="Confirmar contraseña">
		<button>Regitro</button>
	</form>
	<form action="/upload" method="POST" enctype="multipart/form-data">
		<input type="file" name="archivo" id="1">
		<button>subir</button>
	</form>
</div>
<div class="container">
	<div class="form-row">
		<div class="form-group bmd-form-group col-md-6">
			<label for="inputName" class="bmd-label-floating">Nombre</label>
			<input type="text" name="nombre" id="inputName" class="form-control">
		</div>
		<div class="form-group bmd-form-group col-md-6">
			<label for="inputApellidos" class="bmd-label-floating">Apellidos</label>
			<input type="text" name="apellidos" id="inputApellidos" class="form-control">
		</div>
	</div>
	<div class="form-row">
		<div class="form-group bmd-form-group col">
			<label for="inputUsuario" class="bmd-label-floating">Usuario</label>
			<input type="text" name="usuario" id="inputUsuario" class="form-control">
		</div>
	</div>
	<div class="form-row">
		<div class="form-group bmd-form-group col-md-6">
			<label for="inputPass" class="bmd-label-floating">Contraseña</label>
			<input type="text" name="password" id="inputPass" class="form-control">
		</div>
		<div class="form-group bmd-form-group col-md-6">
			<label for="inputPassConf" class="bmd-label-floating">Confirmar contraseña</label>
			<input type="text" name="" id="inputPassConf" class="form-control">
		</div>
	</div>
</div>
<button id="btnTest">Test</button>